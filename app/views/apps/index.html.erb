<h1>Listing apps</h1>

<table>
  <tr>
    <th>ID</th>
		<th>Name</th>
    <th>Ios url</th>
    <th>Android url</th>
    <th>Blackberry url</th>
    <th>Other url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @apps.each do |app| %>
  <tr>
    <td><%= app.id %></td>
		<td><%= app.name %></td>
    <td><%= app.ios_url %></td>
    <td><%= app.android_url %></td>
    <td><%= app.blackberry_url %></td>
    <td><%= app.other_url %></td>
    <td><%= link_to 'Show', app %></td>
    <td><%= link_to 'Edit', edit_app_path(app) %></td>
    <td><%= link_to 'Destroy', app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App', new_app_path %>
